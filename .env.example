# Copy to .env (do not commit) and fill in.

# Postgres password for the docker db container.
POSTGRES_PASSWORD=monarch

# REQUIRED: WinRM credentials used to connect to Windows AV VMs.
MONARCH_WINRM_USER=Administrator
MONARCH_WINRM_PASS=CHANGE_ME

# Recommended: keep logins stable after restart.
MONARCH_COOKIE_SECRET=CHANGE_ME_TO_A_LONG_RANDOM_STRING

# Optional admin panel.
MONARCH_ADMIN_USER=
MONARCH_ADMIN_PASS=

# Optional tuning
MONARCH_WORKERS=2
MONARCH_SCAN_WAIT=15s
MONARCH_MAX_UPLOAD_BYTES=36700160
